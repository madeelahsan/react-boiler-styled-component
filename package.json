{
  "name": "react-boiler-atomic-styled-components",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "lint:all": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "lint:js": "eslint --ext jsx,js src/",
    "lint:js:fix": "eslint --ext jsx,js --fix src/",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@reach/dialog": "^0.15.3",
    "@reach/listbox": "^0.16.1",
    "@reach/tabs": "^0.16.1",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.24.0",
    "moment": "^2.29.1",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-date-range": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-image-crop": "^9.0.5",
    "react-image-file-resizer": "^0.4.7",
    "react-loading-skeleton": "^2.2.0",
    "react-onclickoutside": "^6.11.2",
    "react-popper": "^2.2.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-select": "^4.3.1",
    "react-spring": "^9.2.4",
    "react-toastify": "^7.0.4",
    "react-tooltip": "^4.2.21",
    "styled-components": "^5.2.1",
    "uuidv4": "^6.2.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
